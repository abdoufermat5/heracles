# =============================================================================
# Heracles Systems Schema - LDIF Format
# =============================================================================
# For loading into OpenLDAP via cn=config
# Convert from heracles-systems.schema
# =============================================================================

dn: cn=heracles-systems,cn=schema,cn=config
objectClass: olcSchemaConfig
cn: heracles-systems
#
# Attribute Definitions
#
olcAttributeTypes: ( 1.3.6.1.4.1.99999.2.1.1 
  NAME 'hrcMode'
  DESC 'Heracles - System lock mode: "locked" or "unlocked"'
  EQUALITY caseIgnoreIA5Match
  SUBSTR caseIgnoreIA5SubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )
#
olcAttributeTypes: ( 1.3.6.1.4.1.99999.2.1.10 
  NAME 'hrcPrinterWindowsInfFile'
  DESC 'Heracles - Path to Windows INF file for this printer'
  EQUALITY caseIgnoreIA5Match
  SUBSTR caseIgnoreIA5SubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
#
olcAttributeTypes: ( 1.3.6.1.4.1.99999.2.1.11 
  NAME 'hrcPrinterWindowsDriverDir'
  DESC 'Heracles - Path to Windows driver directory for this printer'
  EQUALITY caseIgnoreIA5Match
  SUBSTR caseIgnoreIA5SubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
#
olcAttributeTypes: ( 1.3.6.1.4.1.99999.2.1.12 
  NAME 'hrcPrinterWindowsDriverName'
  DESC 'Heracles - Windows driver name for this printer'
  EQUALITY caseIgnoreIA5Match
  SUBSTR caseIgnoreIA5SubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
#
olcAttributeTypes: ( 1.3.6.1.4.1.99999.2.1.20 
  NAME 'hrcMobileIMEI'
  DESC 'Heracles - IMEI number of mobile device'
  EQUALITY numericStringMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.36
  SINGLE-VALUE )
#
olcAttributeTypes: ( 1.3.6.1.4.1.99999.2.1.21 
  NAME 'hrcMobileOS'
  DESC 'Heracles - Operating system of mobile device'
  EQUALITY caseExactIA5Match
  SUBSTR caseExactIA5SubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )
#
olcAttributeTypes: ( 1.3.6.1.4.1.99999.2.1.22 
  NAME 'hrcMobilePUK'
  DESC 'Heracles - PUK code for mobile device SIM'
  EQUALITY numericStringMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.36
  SINGLE-VALUE )
#
# ObjectClass Definitions
#
olcObjectClasses: ( 1.3.6.1.4.1.99999.2.2.1 
  NAME 'hrcServer'
  DESC 'Heracles - Server system'
  SUP top
  STRUCTURAL
  MUST ( cn )
  MAY ( hrcMode $ l $ description ) )
#
olcObjectClasses: ( 1.3.6.1.4.1.99999.2.2.2 
  NAME 'hrcWorkstation'
  DESC 'Heracles - Workstation system'
  SUP top
  STRUCTURAL
  MUST ( cn )
  MAY ( hrcMode $ l $ description ) )
#
olcObjectClasses: ( 1.3.6.1.4.1.99999.2.2.3 
  NAME 'hrcTerminal'
  DESC 'Heracles - Terminal system'
  SUP top
  STRUCTURAL
  MUST ( cn )
  MAY ( hrcMode $ l $ description ) )
#
olcObjectClasses: ( 1.3.6.1.4.1.99999.2.2.4 
  NAME 'hrcPrinter'
  DESC 'Heracles - Printer system'
  SUP top
  STRUCTURAL
  MUST ( cn )
  MAY ( hrcMode $ l $ description $ labeledURI $ hrcPrinterWindowsInfFile $ hrcPrinterWindowsDriverDir $ hrcPrinterWindowsDriverName ) )
#
olcObjectClasses: ( 1.3.6.1.4.1.99999.2.2.5 
  NAME 'hrcPhone'
  DESC 'Heracles - Phone system'
  SUP top
  STRUCTURAL
  MUST ( cn )
  MAY ( description $ telephoneNumber $ serialNumber ) )
#
olcObjectClasses: ( 1.3.6.1.4.1.99999.2.2.6 
  NAME 'hrcMobilePhone'
  DESC 'Heracles - Mobile phone'
  SUP top
  STRUCTURAL
  MUST ( cn )
  MAY ( description $ telephoneNumber $ serialNumber $ hrcMobileIMEI $ hrcMobileOS $ hrcMobilePUK ) )
